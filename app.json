{
  "expo": {
    "name": "Dermalert",
    "slug": "Dermalert",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dermalert",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#273161"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#273161"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa acessar suas fotos.",
          "cameraPermission": "O aplicativo precisa acessar sua camera."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "O aplicativo precisa acessar sua camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
